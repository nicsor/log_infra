<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intel.amtl"
    android:versionCode="1"
    android:versionName="2.0"
    android:sharedUserId="android.uid.system" >

    <uses-sdk android:minSdkVersion="10" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:name="Modem_Application">
        <activity
            android:label="@string/app_name"
            android:name=".Main_Activity"
            android:screenOrientation="portrait" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Settings_Activity" android:label="Advanced Menu: Settings" android:screenOrientation="portrait"/>
        <activity android:name=".Additional_Features_Activity" android:label="Advanced Menu: Additional Features" android:screenOrientation="portrait"/>
    </application>
</manifest>